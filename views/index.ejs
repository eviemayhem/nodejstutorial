<!DOCTYPE html>
<html lang="en">

<%- include('./partials/head.ejs') %>

<body>



  <%- include('./partials/nav.ejs') %>

  <div class="blogs content">
    <h2>All Blogs</h2>

    <% if (blogs.length > 0){ %>
    <% blogs.forEach(blogs => {%>
    <h3 class="title"><%= blogs.title %></h3>
    <p class="snippet"></p><%= blogs.snippet %></p>

    <% }) %>
    <% } else {%>
    <p>No blogs yet</p>
    <% } %>



  </div>
</body>
<%- include('./partials/footer.ejs') %>
</html>